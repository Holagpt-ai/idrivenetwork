generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Appointment {
  id                 String   @id @default(cuid())
  name               String
  email              String
  phone              String
  preferredDate      DateTime
  preferredTimeWindow String
  notes              String?
  createdAt          DateTime @default(now())
}
